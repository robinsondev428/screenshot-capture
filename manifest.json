{
    "name": "SelectionBar",
    "version": "3.1",

    "manifest_version": 2,
    "description": "This extension add a powerful bar when you selecting text. Capture, Translate, Share, Highlight, and... ",
    "icons": {
        "48": "images/icon48sb.png",
        "128": "images/icon128sb.png"
    },
    "background": {
        "page": "background.html"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "images/icon19sb.png",
        "default_title": "Webpage Screenshot Bar",
        "default_popup": "popupsb.html"
    },

    "web_accessible_resources": [
        "pluginDev/icons/googleimages.png","pluginDev/icons/copy.png","pluginDev/icons/blogger.png","pluginDev/icons/email.png","pluginDev/icons/evernote.png","pluginDev/icons/facebook.png","pluginDev/icons/gmail.png","pluginDev/icons/googledrive.png","pluginDev/icons/gplus.png","pluginDev/icons/microsoft.png","pluginDev/icons/myspace.png","pluginDev/icons/newwindow.png","pluginDev/icons/pinterest.png","pluginDev/icons/print.png","pluginDev/icons/qrcode.png","pluginDev/icons/search.png","pluginDev/icons/tineye.png","pluginDev/icons/translate.png","pluginDev/icons/twitter.png","pluginDev/icons/vk.png","pluginDev/icons/webpagescreenshot.png","pluginDev/icons/wordpress.png","pluginDev/icons/crop.png","pluginDev/icons/speak.png","pluginDev/icons/share.png","pluginDev/icons/tumblr.png","pluginDev/icons/highlight.png","pluginDev/icons/enlarge.png","pluginDev/icons/ensmall.png","pluginDev/icons/erase.png","pluginDev/icons/edittext.png","pluginDev/icons/help.png","pluginDev/icons/left.png","pluginDev/icons/right.png","pluginDev/icons/del.png","pluginDev/icons/digg.png","pluginDev/icons/dropbox.png","pluginDev/icons/save.png","gapi.js","pluginDev/icons/fast.png","pluginDev/icons/pitch.png","pluginDev/jquery-ui-1.9.2.custom.min.css","SelectionBar/pagestyles.css"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.googleapis.com https://ad.yieldmanager.com https://cdn.extensionanalytics.com https://ssl.google-analytics.com; object-src 'self'",
    "default_locale": "en",
    "optional_permissions": ["<all_urls>","webNavigation", "webRequest", "tabs","http://*/*", "https://*/*","pageCapture", "clipboardWrite"]
}   