{
    "name": "__MSG_name__",
    "version": "14.4",

    "commands": {
        "visible_screenshot": {
            "description": "visible screenshot"
        },
        "entire_page_screenshot": {
            "description": "Entire Page Screenshot"
        }
    },


    "homepage_url": "http://www.webpagescreenshot.info",
    "manifest_version": 2,
    "description": "__MSG_description__",
    "icons": {
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "background": {
        "page": "background.html"
    },
    "default_locale": "en",
    "browser_action": {
        "default_icon": "images/browseraction.png",
        "default_title": "Webpage Screenshot",
        "default_popup": "popup.html"
    },
    "options_page": "options.html",
    "web_accessible_resources": [
        "/images/googleimages.png",
        "/images/copy.png",
        "/images/ajax-20.png",
        "/images/clipboard-paste.png",

        "/images/blogger.png",
        "/images/email.png",
        "/images/evernote.png",
        "/images/facebook.png",
        "/images/gmail.png",
        "/images/googledrive.png",
        "/images/gplus.png",
        "/images/microsoft.png",
        "/images/myspace.png",
        "/images/newwindow.png",
        "/images/pinterest.png",
        "/images/print.png",
        "/images/qrcode.png",
        "/images/search.png",
        "/images/tineye.png",
        "/images/translate.png",
        "/images/twitter.png",
        "/images/vk.png",
        "/images/webpagescreenshot.png",
        "/images/wordpress.png",
        "/images/crop.png",
        "/images/speak.png",
        "/images/share.png",
        "/images/tumblr.png",
        "/images/uploady.png",



        "/images/highlight.png",
        "/images/enlarge.png",
        "/images/ensmall.png",
        "/images/erase.png",
        "/images/edittext.png",
        "/images/help.png",
        "/images/left.png",
        "/images/right.png",
        "/images/del.png",
        "/images/digg.png",
        "/images/dropbox.png",
        "/images/save.png",
        "/images/copy-text.png",
        "/images/link-go.png",

        "/images/fast.png",
        "/images/pitch.png",
        "/libs/jquery-ui-1.9.2.custom.min.css",
        "/css/pagestyles.css",
        "/css/dialog.css",

        "/libs/gapi.js",
        "/libs/jquery.js",
        "/js/dialog.js"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval' https://www.googleapis.com https://cdn.extensionanalytics.com https://ssl.google-analytics.com; object-src 'self'",
    "default_locale": "en",
    "permissions": ["tts","notifications","activeTab","storage", "unlimitedStorage", "webRequest"],
    "optional_permissions": [
    "desktopCapture", "tabs",
     "file://*/*", "http://*/*", "https://*/*", "pageCapture", "tabCapture","webNavigation","clipboardWrite","<all_urls>"
     ]
}